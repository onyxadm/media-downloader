{
  "name": "@w3nder/media-downloader",
  "version": "1.0.6",
  "description": "A simple library to decrypt and download media files directly from message payloads.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "example": "tsx examples/download-media.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3nder/media-downloader.git"
  },
  "keywords": [
    "media",
    "decryption",
    "download",
    "chat"
  ],
  "author": "Wender Teixeira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w3nder/media-downloader/issues"
  },
  "homepage": "https://github.com/w3nder/media-downloader#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "types": "./dist/index.d.ts"
}
